oductsBySell = () => {\r\n    getProducts(\"sold\").then((data) => {\r\n      if (data.error) {\r\n        setError(data.error);\r\n      } else {\r\n        setProductsBySell(data);\r\n      }\r\n    });\r\n  };\r\n\r\n  const loadProductsByArrival = () => {\r\n    getProducts(\"createdAt\").then((data) => {\r\n      if (data.error) {\r\n        setError(data.error);\r\n      } else {\r\n        setProductsByArrival(data);\r\n      }\r\n    });\r\n  };\r\n\r\n  useEffect(() => {\r\n    loadProductsBySell();\r\n    loadProductsByArrival();\r\n  }, []);\r\n\r\n  return (\r\n    <Layout\r\n      title=\"FullStack React Node MongoDB Ecommerce App\"\r\n      description=\"Node React E-commerce App\"\r\n      className=\"container-fluid\"\r\n    >\r\n      <Search />\r\n      <h2>Best Sellers Products</h2>\r\n      <div className=\"row\">\r\n        {productsBySell.map((product, i) => (\r\n          <div key={i} className=\"col-4 mb-3\">\r\n            <Card product={product} />\r\n          </div>\r\n        ))}\r\n      </div>\r\n      <hr />\r\n      <h2>Best Arrivals Products</h2>\r\n      <div className=\"row\">\r\n        {productsByArrival.map((product, i) => (\r\n          <div key={i} className=\"col-4 mb-3\">\r\n            <Card product={product} />\r\n          </div>\r\n        ))}\r\n      </div>\r\n    </Layout>\r\n  );\r\n};\r\n\r\nexport default Home;\r\n","C:\\Users\\nzeuk\\OneDrive\\Documents\\Ecommerce-NodeJs-React-Redux\\frontend\\src\\components\\core\\Menu.js",[],"C:\\Users\\nzeuk\\OneDrive\\Documents\\Ecommerce-NodeJs-React-Redux\\frontend\\src\\components\\core\\Layout.js",[],"C:\\Users\\nzeuk\\OneDrive\\Documents\\Ecommerce-NodeJs-React-Redux\\frontend\\src\\config.js",[],"C:\\Users\\nzeuk\\OneDrive\\Documents\\Ecommerce-NodeJs-React-Redux\\frontend\\src\\components\\auth\\index.js",[],"C:\\Users\\nzeuk\\OneDrive\\Documents\\Ecommerce-NodeJs-React-Redux\\frontend\\src\\components\\user\\UserDashboard.js",["147","148","149"],"C:\\Users\\nzeuk\\OneDrive\\Documents\\Ecommerce-NodeJs-React-Redux\\frontend\\src\\components\\auth\\PrivateRoute.js",["150"],"import React, { Component } from \"react\";\r\nimport { Route, Redirect } from \"react-router-dom\";\r\nimport { isAuthenticated } from \"./index\";\r\n\r\nconst PrivateRoute = ({ component: Component, ...rest }) => (\r\n    <Route\r\n        {...rest}\r\n        render={props =>\r\n            isAuthenticated() ? (\r\n                <Component {...props} />\r\n            ) : (\r\n                <Redirect\r\n                    to={{\r\n                        pathname: \"/signin\",\r\n                        state: { from: props.location }\r\n                    }}\r\n                />\r\n            )\r\n        }\r\n    />\r\n);\r\n\r\nexport default PrivateRoute;","C:\\Users\\nzeuk\\OneDrive\\Documents\\Ecommerce-NodeJs-React-Redux\\frontend\\src\\components\\auth\\AdminRoute.js",["151"],"import React, { Component } from \"react\";\r\nimport { Route, Redirect } from \"react-router-dom\";\r\nimport { isAuthenticated } from \"./index\";\r\n\r\nconst AdminRoute = ({ component: Component, ...rest }) => (\r\n    <Route\r\n        {...rest}\r\n        render={props =>\r\n            isAuthenticated() && isAuthenticated().user.role === 1 ? (\r\n                <Component {...props} />\r\n            ) : (\r\n                <Redirect\r\n                    to={{\r\n                        pathname: \"/signin\",\r\n                        state: { from: props.location }\r\n                    }}\r\n                />\r\n            )\r\n        }\r\n    />\r\n);\r\n\r\nexport default AdminRoute;","C:\\Users\\nzeuk\\OneDrive\\Documents\\Ecommerce-NodeJs-React-Redux\\frontend\\src\\components\\admin\\AddCategory.js",[],"C:\\Users\\nzeuk\\OneDrive\\Documents\\Ecommerce-NodeJs-React-Redux\\frontend\\src\\components\\admin\\ApiAdmin.js",[],"C:\\Users\\nzeuk\\OneDrive\\Documents\\Ecommerce-NodeJs-React-Redux\\frontend\\src\\components\\admin\\AddProduct.js",["152","153","154","155"],"C:\\Users\\nzeuk\\OneDrive\\Documents\\Ecommerce-NodeJs-React-Redux\\frontend\\src\\components\\core\\ApiCore.js",[],"C:\\Users\\nzeuk\\OneDrive\\Documents\\Ecommerce-NodeJs-React-Redux\\frontend\\src\\components\\core\\Card.js",[],"C:\\Users\\nzeuk\\OneDrive\\Documents\\Ecommerce-NodeJs-React-Redux\\frontend\\src\\components\\core\\ShowImage.js",[],"C:\\Users\\nzeuk\\OneDrive\\Documents\\Ecommerce-NodeJs-React-Redux\\frontend\\src\\components\\core\\Shop.js",["156","157","158","159"],"import React, { useState, useEffect } from \"react\";\r\nimport { Link } from \"react-router-dom\";\r\nimport Layout from \"./Layout\";\r\nimport { getCategories } from \"../admin/ApiAdmin\";\r\nimport { getFilteredProducts } from \"./ApiCore\";\r\nimport Checkbox from \"./Checkbox\";\r\nimport { prices } from \"./fixedPrices\";\r\nimport Radiobox from \"./Radiobox\";\r\nimport Card from \"./Card\";\r\n\r\nconst Shop = () => {\r\n  const [myFilters, setMyFilters] = useState({\r\n    filters: { category: [], price: [] },\r\n  });\r\n  const [categories, setCategories] = useState([]);\r\n  const [error, setError] = useState(false);\r\n  const [limit, setLimit] = useState(6);\r\n  const [skip, setSkip] = useState(0);\r\n  const [size, setSize] = useState(0);\r\n  const [filteredResults, setFilteredResults] = useState([]);\r\n\r\n  useEffect(() => {\r\n    init();\r\n    loadFilteredResults(myFilters.filters);\r\n  }, []);\r\n\r\n  const handleFilters = (filters, filterBy) => {\r\n    console.log(\"Parent: \", filters, filterBy);\r\n    const newFilters = { ...myFilters };\r\n    newFilters.filters[filterBy] = filters;\r\n\r\n    if (filterBy === \"price\") {\r\n      let priceValues = handlePrice(filters);\r\n      newFilters.filters[filterBy] = priceValues;\r\n    }\r\n    loadFilteredResults(myFilters.filters);\r\n    setMyFilters(newFilters);\r\n  };\r\n\r\n  const loadFilteredResults = (filters) => {\r\n    getFilteredProducts(skip, limit, filters).then((data) => {\r\n      if (data.error) {\r\n        setError(data.error);\r\n      } else {\r\n        setFilteredResults(data.data);\r\n        setSize(data.size);\r\n        setSkip(0);\r\n      }\r\n    });\r\n  };\r\n\r\n  const loadMore = () => {\r\n    let toSkip = skip + limit;\r\n    getFilteredProducts(toSkip, limit, myFilters.filters).then((data) => {\r\n      if (data.error) {\r\n        setError(data.error);\r\n      } else {\r\n        setFilteredResults([...filteredResults, ...data.data]);\r\n        setSize(data.size);\r\n        setSkip(0);\r\n      }\r\n    });\r\n  };\r\n\r\n  const loadMoreButton = () => {\r\n    return (\r\n      size > 0 &&\r\n      size >= limit && (\r\n        <div className=\"text-center\">\r\n          <hr />\r\n          <button onClick={loadMore} className=\"btn btn-warning mb-5\">\r\n            <i className=\"fa fa-spinner fa-spin\"></i> Load more\r\n          </button>\r\n        </div>\r\n      )\r\n    );\r\n  };\r\n\r\n  const handlePrice = (value) => {\r\n    const data = prices;\r\n    let array = [];\r\n\r\n    for (let key in data) {\r\n      if (data[key]._id === parseInt(value)) {\r\n        array = data[key].array;\r\n      }\r\n    }\r\n    return array;\r\n  };\r\n\r\n  const init = () => {\r\n    getCategories().then((data) => {\r\n      if (data.error) {\r\n        setError(data.error);\r\n      } else {\r\n        setCategories(data);\r\n      }\r\n    });\r\n  };\r\n\r\n  const errorSearch = () => (\r\n    <div className=\"col text-center my-5\">\r\n      <div className=\"ecommerce-shop-no-result-content\">\r\n        <i className=\"fa fa-frown-o\"></i>\r\n        <span>No result found about your search. Try again !!</span>\r\n      </div>\r\n    </div>\r\n  );\r\n\r\n  return (\r\n    <Layout\r\n      title=\"Shop Page\"\r\n      description=\"Search and find books of your choice\"\r\n      className=\"container-fluid\"\r\n    >\r\n      <div className=\"row\">\r\n        <div className=\"col-2\">\r\n          <h4>Filter by categories</h4>\r\n          <ul>\r\n            <Checkbox\r\n              categories={categories}\r\n              handleFilters={(filters) => handleFilters(filters, \"category\")}\r\n            />\r\n          </ul>\r\n          <h4>Filter by Price Range</h4>\r\n          <ul>\r\n            <Radiobox\r\n              prices={prices}\r\n              handleFilters={(filters) => handleFilters(filters, \"price\")}\r\n            />\r\n          </ul>\r\n        </div>\r\n        <div className=\"col-10\">\r\n          <h2 className=\"mb-4\">Products</h2>\r\n          <div className=\"row\">\r\n            {filteredResults.map((product, i) => (\r\n              <div key={i} className=\"col-4 mb-3\">\r\n                <Card product={product} />\r\n              </div>\r\n            ))}\r\n            {filteredResults.length === 0 && errorSearch()}\r\n          </div>\r\n          {loadMoreButton()}\r\n        </div>\r\n      </div>\r\n    </Layout>\r\n  );\r\n};\r\n\r\nexport default Shop;\r\n","C:\\Users\\nzeuk\\OneDrive\\Documents\\Ecommerce-NodeJs-React-Redux\\frontend\\src\\components\\core\\Checkbox.js",["160"],"import React, { useState, useEfect } from \"react\";\r\n\r\nconst Checkbox = ({ categories, handleFilters }) => {\r\n  const [checked, setChecked] = useState([]);\r\n\r\n  const handleToggle = (category) => () => {\r\n    const currentCategoryId = checked.indexOf(category);\r\n    const newCheckedCategoryId = [...checked];\r\n    // if currently checked was not already in checked state > push\r\n    // else pull/take off\r\n    if (currentCategoryId === -1) {\r\n      newCheckedCategoryId.push(category);\r\n    } else {\r\n      newCheckedCategoryId.splice(currentCategoryId, 1);\r\n    }\r\n    console.log(currentCategoryId)\r\n    console.log(newCheckedCategoryId)\r\n    setChecked(newCheckedCategoryId)\r\n    handleFilters(newCheckedCategoryId)\r\n  };\r\n\r\n  return categories.map((category, index) => (\r\n    <li key={index} className=\"